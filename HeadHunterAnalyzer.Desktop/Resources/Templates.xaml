<ResourceDictionary 
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:viewModels="clr-namespace:HeadHunterAnalyzer.Desktop.ViewModels"
	xmlns:views="clr-namespace:HeadHunterAnalyzer.Desktop.Views"
	
	xmlns:navigationLayoutViewModel="clr-namespace:HeadHunterAnalyzer.Desktop.ViewModels.Layouts.MainPage"
	xmlns:navigationLayout="clr-namespace:HeadHunterAnalyzer.Desktop.Views.Layouts">

	<DataTemplate DataType="{x:Type viewModels:MainPageViewModel}">
		<views:MainPageView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type viewModels:AnalyzeVacancyViewModel}">
		<views:AnalyzeVacancyView />
	</DataTemplate>

	<DataTemplate DataType="{x:Type navigationLayoutViewModel:MainPageNavigationLayoutViewModel}">
		<navigationLayout:MainPageNavigationLayout />
	</DataTemplate>

</ResourceDictionary>